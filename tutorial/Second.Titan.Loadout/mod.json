{
	"Name": "Second Titan Loadout",
	"Description": "Replaces your titan loadout",
	"LoadPriority": 1,
	"RequiredOnClient": false,
	"Version": "1.0.0",

	"Scripts": [
		{
			"Path": "titan_second_loadout.gnut",			// 你的脚本相对于“vscripts”文件夹的路径
			"RunOn": "SERVER && MP",						// 你的脚本在什么情况下运行。SERVER && MP 即在服务器环境且为多人模式时
			"ServerCallback": {
				"After": "SecondTitanLoadout_Init"			// 你的脚本在游戏内脚本加载完毕后，是否调用一个函数。这里就调用主函数
			}
		}
	]
}